executable ship
  default-language:
    Haskell2010
  ghc-options:
    -threaded
    -Wall
    -Werror
    -fwarn-tabs
    -funbox-strict-fields
    -O2
    -fno-warn-orphans
    -fno-warn-unused-do-bind
    -with-rtsopts=-N
  main-is:
    Main.hs
